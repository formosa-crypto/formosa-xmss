from XMSS require "hash_address/hash_address.jinc"

export fn set_layer_addr_jazz(
    reg ptr u32[8] addr, 
    #transient reg u32 layer
) -> reg ptr u32[8] 
{
    _ = #init_msf();

    addr = __set_layer_addr(addr, layer);

    return addr;
}

