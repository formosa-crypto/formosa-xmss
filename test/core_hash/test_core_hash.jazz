from XMSS require "params.jinc"
from XMSS require "core_hash/core_hash_shake256_out_64.jtmpl"

// NOTE: Outlen is 64

export fn core_hash<INLEN>(
    reg ptr u8[64] out,
    reg ptr u8[INLEN] in
) -> reg ptr u8[64] 
{
    out = __core_hash_<64, INLEN>(out, in); 
    return out;
}//<>

export fn core_hash_in_ptr(
    reg ptr u8[64] out,
    reg u64 in_ptr inlen
) -> reg ptr u8[64] 
{
    out = _core_hash_in_ptr<64>(out, in_ptr, inlen);
    return out;
}
